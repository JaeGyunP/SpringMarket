<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="good">

	<select id="goodlist"
		resultType="com.example.springmarket.model.good.GoodDTO">
		select * from good
	</select>

<select id ="detailgood" resultType="com.example.springmarket.model.good.GoodDTO">

select * from good where goodidx = #{goodidx}
</select>

</mapper>